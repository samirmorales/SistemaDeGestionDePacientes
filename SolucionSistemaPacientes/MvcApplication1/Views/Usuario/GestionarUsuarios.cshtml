@model MvcApplication1.Models.usuarioLoginModel

@{
    Layout = "~/Views/MasterIntranet.cshtml";
    ViewBag.Title = "GestionarUsuarios";
}


<section class="content-header">
    <h1>
        Gestionar Usuarios
        <small>Buscar, modificar y eliminar usuarios.</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-group"></i> Inicio</a></li>
        <li><a href="#"><i></i> Usuarios</a></li>
        <li class="active">Gestionar Usuarios</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <div class="row">
        <!-- left column -->
        <div class="col-md-12">
            @if (@ViewBag.mensaje!=null)
            {
                <div class="alert alert-success alert-dismissable">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                    <h4><i class="icon fa fa-check"></i> @ViewBag.mensaje</h4>
                </div>
            }
        </div>    
     </div>        
    <div class="row">
            <!-- left column -->
            <div class="col-md-12">
                <!-- solid sales graph -->
              <div class="box box-default box-solid collapsed-box">
                <div class="box-header with-border">
                  <i class="fa fa-user"></i>
                  <h3 class="box-title">Administradores - Usuarios</h3>
                  <div class="box-tools pull-right">
                    <button class="btn bg-black btn-sm" data-widget="collapse"><i class="fa fa-plus"></i></button>
                  </div>
                </div>
                <div class="box-body border-radius-none">
                  <table id="tablaAdministradorUsuario" class="table table-bordered table-striped">
                    <thead>
                      <tr>
                        <th>#</th>
                        <th>Médico</th>
                        <th>Usuario</th>
                        <th>Especialidad</th>
                        <th>Email</th>
                        <th>Estado</th>
                      </tr>
                    </thead>
                    <tbody>
                       
                    </tbody>
                  </table>
                </div><!-- /.box-body -->
                <div class="box-footer no-border">
                  <div class="row">
                    <div class="col-xs-4 text-center" style="border-right: 1px solid #f4f4f4">
                      
                    </div><!-- ./col -->
                    <div class="col-xs-4 text-center" style="border-right: 1px solid #f4f4f4">
                      
                    </div><!-- ./col -->
                    <div class="col-xs-4 text-center">
                      
                    </div><!-- ./col -->
                  </div><!-- /.row -->
                </div><!-- /.box-footer -->
              </div><!-- /.box -->

           </div><!-- /.col6 -->

           <div class="col-md-6">
              <!-- Horizontal Form -->
              
           </div><!-- /.col6 -->
        </div><!-- /.row -->
        <div class="row">
            <!-- left column -->
            <div class="col-md-12">
                <!-- solid sales graph -->
              <div class="box box-success box-solid collapsed-box">
                <div class="box-header with-border" >
                  <i class="fa fa-female"></i>
                  <h3 class="box-title">Secretarias - Usuarios</h3>
                  <div class="box-tools pull-right">
                    <button class="btn bg-black btn-sm" data-widget="collapse"><i class="fa fa-plus"></i></button>
                  </div>
                </div>
                <div class="box-body">
                  <table id="tablaSecretariaUsuario" class="table table-bordered table-striped">
                    <thead>
                      <tr>
                        <th>#</th>
                        <th>Médico</th>
                        <th>Usuario</th>
                        <th>Especialidad</th>
                        <th>Email</th>
                        <th>Estado</th>
                      </tr>
                    </thead>
                    <tbody>
                       
                    </tbody>
                  </table>
                </div><!-- /.box-body -->
                <div class="box-footer no-border">
                  <div class="row">
                    <div class="col-xs-4 text-center" style="border-right: 1px solid #f4f4f4">
                      
                    </div><!-- ./col -->
                    <div class="col-xs-4 text-center" style="border-right: 1px solid #f4f4f4">
                      
                    </div><!-- ./col -->
                    <div class="col-xs-4 text-center">
                      
                    </div><!-- ./col -->
                  </div><!-- /.row -->
                </div><!-- /.box-footer -->
              </div><!-- /.box -->

           </div><!-- /.col6 -->

           <div class="col-md-6">
              <!-- Horizontal Form -->
              
           </div><!-- /.col6 -->
        </div><!-- /.row -->
        <div class="row">
            <!-- left column -->
            <div class="col-md-12">
                <!-- solid sales graph -->
              <div class="box box-danger box-solid collapsed-box">
                <div class="box-header with-border">
                  <i class="fa fa-heartbeat"></i>
                  <h3 class="box-title">Medicos - Usuarios</h3>
                  <div class="box-tools pull-right">
                    <button class="btn bg-black btn-sm" data-widget="collapse"><i class="fa fa-plus"></i></button>
                  </div>
                </div>
                <div class="box-body border-radius-none">
                  <table id="tablaMedicoUsuario" class="table table-bordered table-striped">
                    <thead>
                      <tr>
                        <th>#</th>
                        <th>Médico</th>
                        <th>Usuario</th>
                        <th>Especialidad</th>
                        <th>DNI</th>
                        <th>Email</th>
                        <th>Opc</th>
                      </tr>
                    </thead>
                    <tbody>
                       @{
                           int i = 0;
                        }
                        @foreach (var e in (List<Entidades.entMedico>)ViewData["ListaMedicosUsuario"])
                            {
                             i++;
                             <tr>
                                 <td>@i</td>
                                 <td>@Html.DisplayFor(modelItem => e.nombre) @Html.DisplayFor(modelItem => e.apellidoPaterno)</td>
                                 <td>@Html.DisplayFor(modelItem => e.usuario)</td>
                                 <td>@Html.DisplayFor(modelItem => e.especialidad)</td>
                                 <td>@Html.DisplayFor(modelItem => e.dni)</td>
                                 <td>@Html.DisplayFor(modelItem => e.email)</td>
                                 <td>
                                     <a href="@Url.Action("EditarMedico", "Medico", new { idMedico = e.idMedico})" title="Editar Médico" rel="facebox"><i class="fa fa-edit"></i></a>&nbsp;&nbsp;&nbsp;
                                     <a href="@Url.Action("EliminarMedico", "Medico" , new { idMedico = e.idMedico})"title="Eliminar Médico"><i class="fa fa-times-circle"></i></a>
                                 </td>
                             </tr>
                            }
                    </tbody>
                  </table>
                </div><!-- /.box-body -->
                <div class="box-footer no-border">
                  <div class="row">
                    <div class="col-xs-4 text-center" style="border-right: 1px solid #f4f4f4">
                      
                    </div><!-- ./col -->
                    <div class="col-xs-4 text-center" style="border-right: 1px solid #f4f4f4">
                      
                    </div><!-- ./col -->
                    <div class="col-xs-4 text-center">
                      
                    </div><!-- ./col -->
                  </div><!-- /.row -->
                </div><!-- /.box-footer -->
              </div><!-- /.box -->

           </div><!-- /.col6 -->

           <div class="col-md-6">
              <!-- Horizontal Form -->
              
           </div><!-- /.col6 -->
        </div><!-- /.row -->
        <div class="row">
            <!-- left column -->
            <div class="col-md-12">
                <!-- solid sales graph -->
              <div class="box box-warning box-solid collapsed-box">
                <div class="box-header with-border">
                  <i class="fa fa-bed"></i>
                  <h3 class="box-title">Pacientes - Usuarios</h3>
                  <div class="box-tools pull-right">
                    <button class="btn bg-black btn-sm" data-widget="collapse"><i class="fa fa-plus"></i></button>
                  </div>
