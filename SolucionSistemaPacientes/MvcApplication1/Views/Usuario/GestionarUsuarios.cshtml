                        <th>Email</th>
                        <th>Estado</th>
                      </tr>
                    </thead>
                    <tbody>
                       
                    </tbody>
                  </table>
                </div><!-- /.box-body -->
                <div class="box-footer no-border">
                  <div class="row">
                    <div class="col-xs-4 text-center" style="border-right: 1px solid #f4f4f4">
                      
                    </div><!-- ./col -->
                    <div class="col-xs-4 text-center" style="border-right: 1px solid #f4f4f4">
                      
                    </div><!-- ./col -->
                    <div class="col-xs-4 text-center">
                      
                    </div><!-- ./col -->
                  </div><!-- /.row -->
                </div><!-- /.box-footer -->
              </div><!-- /.box -->

           </div><!-- /.col6 -->

           <div class="col-md-6">
              <!-- Horizontal Form -->
              
           </div><!-- /.col6 -->
        </div><!-- /.row -->
</section><!-- /.content -->






