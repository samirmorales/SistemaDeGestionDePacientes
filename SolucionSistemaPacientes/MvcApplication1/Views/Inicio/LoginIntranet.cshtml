<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title> SISPAC | WEB</title>
    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
    <link href="../Plantilla/AdminLTE-2.1.1/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css" rel="stylesheet" type="text/css" />
    <link href="../Plantilla/AdminLTE-2.1.1/dist/css/AdminLTE.min.css" rel="stylesheet" type="text/css" />
    <link href="../Plantilla/AdminLTE-2.1.1/dist/css/skins/_all-skins.min.css" rel="stylesheet" type="text/css" />
    <link href="../Plantilla/AdminLTE-2.1.1/plugins/iCheck/flat/blue.css" rel="stylesheet" type="text/css" />
    <link href="../Plantilla/AdminLTE-2.1.1/plugins/morris/morris.css" rel="stylesheet" type="text/css" />
    <link href="../Plantilla/AdminLTE-2.1.1/plugins/jvectormap/jquery-jvectormap-1.2.2.css" rel="stylesheet" type="text/css" />
    <link href="../Plantilla/AdminLTE-2.1.1/plugins/datepicker/datepicker3.css" rel="stylesheet" type="text/css" />
    <link href="../Plantilla/AdminLTE-2.1.1/plugins/daterangepicker/daterangepicker-bs3.css" rel="stylesheet" type="text/css" />
    <link href="../Plantilla/AdminLTE-2.1.1/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css" rel="stylesheet" type="text/css" />

</head>
<body class="login-page">
    
    <div class="login-box">
      <div class="login-logo">
        <a href="#"><b>Sis</b>Pac</a>
      </div><!-- /.login-logo -->
      <div class="login-box-body">
        <p class="login-box-msg">Inicio de Sesión</p>
            <form method="post" action="@Url.Action("LoginIntranet", "Inicio")">
              <div class="form-group has-feedback">
                <input type="text" name="txtUsuario" class="form-control" placeholder="usuario" required/>
                <span class="glyphicon glyphicon-envelope form-control-feedback"></span>
              </div>
              <div class="form-group has-feedback">
                <input type="password" name="txtPassword" class="form-control" placeholder="Password" required/>
                <span class="glyphicon glyphicon-lock form-control-feedback"></span>
              </div>
              <div class="row">
                <div class="col-xs-8">    
                  <div class="checkbox icheck">
                    <label>
                      <a href="#">Olvidé mi contraseña.</a><br>
                    </label>
                  </div>                        
                </div><!-- /.col -->
                <div class="col-xs-4">
                  <button type="submit" class="btn btn-primary btn-block btn-flat">Entrar</button>
                </div><!-- /.col -->
              </div>
              <div class="row">
                  <div class="col-xs-12"> 
                      <h3 align="center" style="color:red">@ViewBag.mensaje</h3>
                  </div>
              </div>   
            </form>
      </div><!-- /.login-box-body -->
    </div><!-- /.login-box -->
    <script src="../Plantilla/AdminLTE-2.1.1/plugins/jQuery/jQuery-2.1.4.min.js"></script>

    <script src="http://code.jquery.com/ui/1.11.2/jquery-ui.min.js" type="text/javascript"></script>

    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>

    <script src="../Plantilla/AdminLTE-2.1.1/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>

    <script src="http://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>
    <script src="../Plantilla/AdminLTE-2.1.1/plugins/morris/morris.min.js" type="text/javascript"></script>
    <script src="../Plantilla/AdminLTE-2.1.1/plugins/sparkline/jquery.sparkline.min.js" type="text/javascript"></script>
    <script src="../Plantilla/AdminLTE-2.1.1/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js" type="text/javascript"></script>
    <script src="../Plantilla/AdminLTE-2.1.1/plugins/jvectormap/jquery-jvectormap-world-mill-en.js" type="text/javascript"></script>
    <script src="../Plantilla/AdminLTE-2.1.1/plugins/knob/jquery.knob.js" type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.2/moment.min.js" type="text/javascript"></script>
    <script src="../Plantilla/AdminLTE-2.1.1/plugins/daterangepicker/daterangepicker.js" type="text/javascript"></script>
    <script src="../Plantilla/AdminLTE-2.1.1/plugins/datepicker/bootstrap-datepicker.js" type="text/javascript"></script>
    <script src="../Plantilla/AdminLTE-2.1.1/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js" type="text/javascript"></script>
    <script src="../Plantilla/AdminLTE-2.1.1/plugins/slimScroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src='../Plantilla/AdminLTE-2.1.1/plugins/fastclick/fastclick.min.js'></script>
    <script src="../Plantilla/AdminLTE-2.1.1/dist/js/app.min.js" type="text/javascript"></script>
    <script src="../Plantilla/AdminLTE-2.1.1/dist/js/pages/dashboard.js" type="text/javascript"></script>
    <script src="../Plantilla/AdminLTE-2.1.1/dist/js/demo.js" type="text/javascript"></script>
</body>
</html>